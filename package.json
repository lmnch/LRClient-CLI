{
  "name": "lrclient-cli",
  "version": "0.0.1",
  "description": "CLI wrapper for lrclient (https://github.com/lmnch/lrclient)",
  "author": "lmnch",
  "bin": {
    "lrc": "./bin/run"
  },
  "homepage": "https://github.com/lmnch/lrclient-cli",
  "license": "GPL",
  "main": "dist/index.js",
  "repository": "lmnch/lrclient-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.19.1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.5",
    "cli-select-2": "^2.0.0",
    "lrclient": "file:../LRClient"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.4",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.68",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "jest": "^28.1.3",
    "mocha": "^9",
    "oclif": "^3.2.24",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.8.4"
  },
  "oclif": {
    "bin": "lrc",
    "dirname": "lrc",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/lmnch/lrclient-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "jest": {
    "automock": false,
    "resetMocks": false,
    "setupFiles": [
      "./setupJest.js"
    ]
  }
}
